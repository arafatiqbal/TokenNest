{
  "name": "tokennest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "concurrently \"vite dev\" \"json-server --watch static/mock-data/mock-data.json\"",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest --ui",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@auth/core": "^0.18.1",
    "@auth/sveltekit": "^0.3.10",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.1",
    "@testing-library/svelte": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "daisyui": "^4.4.19",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.4",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.2",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "type": "module"
}
